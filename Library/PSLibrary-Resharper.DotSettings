	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=EAE9C6DCAB176942975887BA1002272B/Categories/=Imported_00202022_002D07_002D25/@EntryIndexedValue">Imported 2022-07-25</s:String>
