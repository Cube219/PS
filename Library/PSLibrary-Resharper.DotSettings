	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=4024803E16C16A41B2744AC6CA2EE5B4/Shortcut/@EntryValue">psbicoeff</s:String>
	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=4024803E16C16A41B2744AC6CA2EE5B4/Description/@EntryValue">Binomial Coefficient</s:String>
	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=4024803E16C16A41B2744AC6CA2EE5B4/Text/@EntryValue">template &lt;int mod&gt;&#xD;
struct BiCoeff&#xD;
{&#xD;
    vector&lt;ll&gt; fact, invfact;&#xD;
&#xD;
    ll pw(ll a, ll b)&#xD;
    {&#xD;
        ll res = 1;&#xD;
        while(b &gt; 0) {&#xD;
            if(b &amp; 1) {&#xD;
                res = res * a % mod;&#xD;
            }&#xD;
            a = a * a % mod;&#xD;
            b &gt;&gt;= 1;&#xD;
        }&#xD;
        return res;&#xD;
    }&#xD;
&#xD;
    void init(int n)&#xD;
    {&#xD;
        fact.resize(n + 1);&#xD;
        invfact.resize(n + 1);&#xD;
&#xD;
        fact[0] = 1;&#xD;
        for(int i = 1; i &lt;= n; ++i) fact[i] = fact[i - 1] * i % mod;&#xD;
        invfact[n] = pw(fact[n], mod - 2);&#xD;
        for(int i = n - 1; i &gt;= 0; --i) invfact[i] = invfact[i + 1] * (i + 1) % mod;&#xD;
    }&#xD;
&#xD;
    ll get(int n, int r)&#xD;
    {&#xD;
        return fact[n] * (invfact[r] * invfact[n - r] % mod) % mod;&#xD;
        // return fact[n] * pw(fact[r] * fact[n - r] % mod, mod - 2) % mod;&#xD;
    }&#xD;
};&#xD;
</s:String>
