	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=8BDAA9169A405045ABD958B06156DE0C/Categories/=Imported_00202022_002D07_002D25/@EntryIndexedValue">Imported 2022-07-25</s:String>
