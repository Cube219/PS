	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=4024803E16C16A41B2744AC6CA2EE5B4/Scope/=F6696F8185506F41B9C4252540BE9C18/Type/@EntryValue">InCppFile</s:String>
	<s:Boolean x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=44A144506BAD8544820BCED09792AB0D/@KeyIndexDefined">True</s:Boolean>
	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=44A144506BAD8544820BCED09792AB0D/Shortcut/@EntryValue">psbicoeff_lucas</s:String>
	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=44A144506BAD8544820BCED09792AB0D/Description/@EntryValue">Binomial Coefficient - Lucas</s:String>
	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=44A144506BAD8544820BCED09792AB0D/Text/@EntryValue">template &lt;int mod&gt;&#xD;
struct BiCoeff_Lucas&#xD;
{&#xD;
    vector&lt;ll&gt; fact, invfact;&#xD;
&#xD;
    ll pw(ll a, ll b)&#xD;
    {&#xD;
        ll res = 1;&#xD;
        while(b &gt; 0) {&#xD;
            if(b &amp; 1) {&#xD;
                res = res * a % mod;&#xD;
            }&#xD;
            a = a * a % mod;&#xD;
            b &gt;&gt;= 1;&#xD;
        }&#xD;
        return res;&#xD;
    }&#xD;
&#xD;
    // O(mod)&#xD;
    void init()&#xD;
    {&#xD;
        int n = mod - 1;&#xD;
        fact.resize(n);&#xD;
        invfact.resize(n);&#xD;
&#xD;
        fact[0] = 1;&#xD;
        for(int i = 1; i &lt;= n; ++i) fact[i] = fact[i - 1] * i % mod;&#xD;
        invfact[n] = pw(fact[n], mod - 2);&#xD;
        for(int i = n - 1; i &gt;= 0; --i) invfact[i] = invfact[i + 1] * (i + 1) % mod;&#xD;
    }&#xD;
&#xD;
    ll ncr(int n, int r)&#xD;
    {&#xD;
        if(n &lt; r) return 0;&#xD;
        ll factn = fact[n];&#xD;
        ll invrnr = invfact[r] * invfact[n - r] % mod;&#xD;
        return factn * invrnr % mod;&#xD;
    }&#xD;
&#xD;
    // O(logn + logr), Only use when MOD is prime&#xD;
    ll get(ll n, ll r)&#xD;
    {&#xD;
        ll res = 1;&#xD;
        while(n &gt; 0 || r &gt; 0) {&#xD;
            ll a = n % mod;&#xD;
            ll b = r % mod;&#xD;
            res *= ncr(a, b);&#xD;
            res %= mod;&#xD;
&#xD;
            n /= mod;&#xD;
            r /= mod;&#xD;
        }&#xD;
        return res;&#xD;
    }&#xD;
};&#xD;
</s:String>
