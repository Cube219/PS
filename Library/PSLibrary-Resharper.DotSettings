	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=524CB0AEC16B0B40BDB0A4A3E50F5088/Description/@EntryValue">Treap</s:String>
	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=524CB0AEC16B0B40BDB0A4A3E50F5088/Text/@EntryValue">struct Treap&#xD;
{&#xD;
    Treap* l, * r;&#xD;
    int pri, sz;&#xD;
    int v;&#xD;
&#xD;
    Treap() = default;&#xD;
    Treap(int _v)&#xD;
    {&#xD;
        l = r = nullptr;&#xD;
        pri = getRandom();&#xD;
        v = _v;&#xD;
        sz = 1;&#xD;
    }&#xD;
    ~Treap()&#xD;
    {&#xD;
        delete l; delete r;&#xD;
    }&#xD;
&#xD;
    void update()&#xD;
    {&#xD;
        sz = 1;&#xD;
        if(l) sz += l-&gt;sz;&#xD;
        if(r) sz += r-&gt;sz;&#xD;
        push();&#xD;
    }&#xD;
&#xD;
    void push()&#xD;
    {&#xD;
    }&#xD;
};&#xD;
&#xD;
// sz == left treap size&#xD;
pair&lt;Treap*, Treap*&gt; split(Treap* rt, int sz)&#xD;
{&#xD;
    if(!rt) return { 0, 0 };&#xD;
&#xD;
    rt-&gt;push();&#xD;
&#xD;
    int lsz = rt-&gt;l ? rt-&gt;l-&gt;sz : 0;&#xD;
    if(lsz + 1 &lt;= sz) {&#xD;
        auto [l, r] = split(rt-&gt;r, sz - lsz - 1);&#xD;
        rt-&gt;r = l;&#xD;
        rt-&gt;update();&#xD;
        return { rt, r };&#xD;
    } else {&#xD;
        auto [l, r] = split(rt-&gt;l, sz);&#xD;
        rt-&gt;l = r;&#xD;
        rt-&gt;update();&#xD;
        return { l, rt };&#xD;
    }&#xD;
}&#xD;
&#xD;
Treap* merge(Treap* l, Treap* r)&#xD;
{&#xD;
    if(!l) return r; if(!r) return l;&#xD;
&#xD;
    l-&gt;push(); r-&gt;push();&#xD;
&#xD;
    if(l-&gt;pri &lt; r-&gt;pri) {&#xD;
        r-&gt;l = merge(l, r-&gt;l);&#xD;
        r-&gt;update();&#xD;
        return r;&#xD;
    } else {&#xD;
        l-&gt;r = merge(l-&gt;r, r);&#xD;
        l-&gt;update();&#xD;
        return l;&#xD;
    }&#xD;
}&#xD;
</s:String>
