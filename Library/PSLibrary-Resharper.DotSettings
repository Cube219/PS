	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=861B6EDE7AE44D489B80466F28A053DB/Categories/=Imported_00202022_002D07_002D25/@EntryIndexedValue">Imported 2022-07-25</s:String>
