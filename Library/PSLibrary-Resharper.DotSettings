	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=7B99A65AAD0EBA4E913C9CD71F39E15E/Text/@EntryValue">struct KMP&#xD;
{&#xD;
    vector&lt;int&gt; fail;&#xD;
&#xD;
    void init_fail(const string&amp; w)&#xD;
    {&#xD;
        int wn = w.size();&#xD;
        fail.clear();&#xD;
        fail.resize(wn, 0);&#xD;
&#xD;
        int j = 0;&#xD;
        for(int i = 1; i &lt; wn; ++i) {&#xD;
            while(j &gt; 0 &amp;&amp; w[i] != w[j]) {&#xD;
                j = fail[j - 1];&#xD;
            }&#xD;
            if(w[i] == w[j]) {&#xD;
                fail[i] = j + 1;&#xD;
                j++;&#xD;
            }&#xD;
        }&#xD;
    }&#xD;
&#xD;
    void get(const string&amp; s, const string&amp; w, vector&lt;int&gt;&amp; res)&#xD;
    {&#xD;
        init_fail(w);&#xD;
        res.clear();&#xD;
&#xD;
        int sn = s.size();&#xD;
        int wn = w.size();&#xD;
&#xD;
        int j = 0;&#xD;
        for(int i = 0; i &lt; sn; ++i) {&#xD;
            while(j &gt; 0 &amp;&amp; s[i] != w[j]) {&#xD;
                j = fail[j - 1];&#xD;
            }&#xD;
            if(s[i] == w[j]) {&#xD;
                j++;&#xD;
            }&#xD;
            if(j == wn) {&#xD;
                res.push_back(i - wn + 1);&#xD;
                j = fail[j - 1];&#xD;
            }&#xD;
        }&#xD;
    }&#xD;
};&#xD;
</s:String>
