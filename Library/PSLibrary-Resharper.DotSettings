	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=C36C6199B9ACDF45AA9D5CF03AD74623/Description/@EntryValue">SCC</s:String>
	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=C36C6199B9ACDF45AA9D5CF03AD74623/Text/@EntryValue">vector&lt;int&gt; d(n, 0), st, finish(n, false), sccIdx(n);&#xD;
vector&lt;vector&lt;int&gt;&gt; sccs;&#xD;
int dNum = 0;&#xD;
&#xD;
auto scc_dfs = [&amp;](auto&amp;&amp; self, int cur) -&gt; int {&#xD;
    d[cur] = ++dNum;&#xD;
    st.push_back(cur);&#xD;
&#xD;
    int ret = d[cur];&#xD;
    for(int nxt : g[cur]) {&#xD;
        if(d[nxt] == 0) ret = min(ret, self(self, nxt));&#xD;
        else if(finish[nxt] == false) ret = min(ret, d[nxt]);&#xD;
    }&#xD;
&#xD;
    if(ret == d[cur]) {&#xD;
        sccs.push_back({});&#xD;
        auto&amp; scc = sccs.back();&#xD;
        while(1) {&#xD;
            int top = st.back();&#xD;
            st.pop_back();&#xD;
&#xD;
            scc.push_back(top);&#xD;
            sccIdx[top] = sccs.size();&#xD;
            finish[top] = true;&#xD;
&#xD;
            if(top == cur) break;&#xD;
        }&#xD;
    }&#xD;
    return ret;&#xD;
};&#xD;
for(int i = 0; i &lt; n; ++i) {&#xD;
    if(d[i] == 0) scc_dfs(scc_dfs, i);&#xD;
}&#xD;
