	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=B08A220A3693E540A893ED70DAE5D357/Scope/=F6696F8185506F41B9C4252540BE9C18/Type/@EntryValue">InCppFile</s:String>
