	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=861B6EDE7AE44D489B80466F28A053DB/Shortcut/@EntryValue">psgeo_lineinter</s:String>
	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=861B6EDE7AE44D489B80466F28A053DB/Description/@EntryValue">Line Interaction</s:String>
	<s:String x:Key="/Default/PatternsAndTemplates/LiveTemplates/Template/=861B6EDE7AE44D489B80466F28A053DB/Text/@EntryValue">// num=0: no inter / num=1: has inter / num=2: inf inter&#xD;
template &lt;typename P&gt;&#xD;
P getLineInter(P a1, P a2, P b1, P b2, int&amp; num)&#xD;
{&#xD;
    P a = a2 - a1, b = b2 - b1;&#xD;
    auto d = a.cross(b);&#xD;
    if(d == 0) {&#xD;
        if(a.cross(b1 - a1) == 0) num = 2;&#xD;
        else num = 0;&#xD;
        return { 0, 0 };&#xD;
    }&#xD;
    auto p = (a2 - b1).cross(b);&#xD;
    auto q = b.cross(a1 - b1);&#xD;
    num = 1;&#xD;
    return (a1 * p + a2 * q) / d;&#xD;
}&#xD;
</s:String>
