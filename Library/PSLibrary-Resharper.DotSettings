    int n = (int)s.size();&#xD;
    vector&lt;int&gt; z(n);&#xD;
    for(int i = 1, l = 0, r = 0; i &lt; n; ++i) {&#xD;
        if(i &lt;= r) z[i] = min(r - i + 1, z[i - l]);&#xD;
        while(i + z[i] &lt; n &amp;&amp; s[z[i]] == s[i + z[i]]) z[i]++;&#xD;
        if(i + z[i] - 1 &gt; r) {&#xD;
            l = i;&#xD;
            r = i + z[i] - 1;&#xD;
        }&#xD;
    }&#xD;
    return z;&#xD;
}&#xD;
